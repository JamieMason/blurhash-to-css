{
  "name": "blurhash-to-css",
  "description": "Convert a BlurHash String to a CSS Object",
  "version": "0.3.1",
  "author": "Jamie Mason <jamie@foldleft.io> (https://github.com/JamieMason)",
  "bugs": "https://github.com/JamieMason/blurhash-to-css/issues",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "jest": "26.6.3",
    "neon-cli": "0.7.1",
    "ts-jest": "26.5.0",
    "typescript": "4.1.3"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "native/index.node"
  ],
  "keywords": [
    "blur",
    "blurhash",
    "hash",
    "image",
    "image loading",
    "image placeholder",
    "largest contentful paint",
    "perfmatters",
    "performance",
    "placeholder",
    "rust",
    "wasm",
    "web vitals",
    "webassembly",
    "webperf"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "manager": "yarn",
  "repository": "JamieMason/blurhash-to-css",
  "scripts": {
    "build": "yarn build:rust && yarn build:ts",
    "build:rust": "neon build --release",
    "build:ts": "tsc --project .",
    "prebuild": "rm -rf dist && neon clean",
    "prepack": "yarn build",
    "test": "yarn test:rust && yarn test:ts",
    "test:rust": "(cd ./native && cargo test)",
    "test:ts": "jest"
  },
  "types": "dist/index.d.ts"
}
